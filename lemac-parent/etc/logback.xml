<?xml version="1.0" encoding="UTF-8" ?>
<configuration>

	<appender name="FILE"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-4relative [%thread] %-5level %class - %msg%n</pattern>
		</encoder>
	</appender>

        <!--Daily rolling file appender -->

	
	
	<root>
		<level value="info" />
		<appender-ref ref="FILE" />
	</root>
</configuration>
